
view{
  box-sizing:border-box;
}

.rank_bg{
  background:#fff;
  min-height:100vh;
  /* padding:40rpx; */
  width:100vw;
  box-sizing:border-box;
}

.rank_chose_week{
  
  width:100%;
  background:#fff;
  height:100rpx;
  line-height:100rpx;
}
.rank_week_main{
  margin:0 auto;
  width:70%;
  height:100rpx;
}
.rank_week{
  height:100rpx;
  line-height:100rpx;
  font-size:30rpx;
  width:50%;
  padding:0 50rpx;
  
  color:#adadad;
  cursor:pointer;
  text-align: center;
}
.rank_week_act{
  /* background:#fff;
  
  color:#318EFD; */
  color:#318EFD;
  border-bottom:3px solid #318EFD;
}
.rank_rule{
  background:linear-gradient(to bottom,#F6F6F6,#FEFEFE);
  height:10px;
  width:100%;
}

.rank_items{
  position: relative;
  font-size:30rpx;
  height:100rpx;
  line-height:100rpx;
}
.rank_items_title{
  color:#aaaaaa;
  font-weight: normal;
}

.rank_items_first{
  position: absolute;
  left:0;
  top:0;
  width:130rpx;
  text-align: center;
} 
.rank_items_bd:nth-child(1) .rank_items_first{
  color:#EBD497;
}
.rank_items_bd:nth-child(2) .rank_items_first{
  color:#C6D0E1;
}
.rank_items_bd:nth-child(3) .rank_items_first{
  color:#D5C3B0;
}
.rank_items_second{
  position: absolute;
  left:140rpx;
  top:10rpx;
  width:80rpx;
  height:80rpx;
}
.rank_items_userbg,.rank_items_userPic{
  
  
  /* background:red; */
}
.rank_items_userbg{
  position: absolute;
  left:0;
  top:-19rpx;
  width:80rpx;
  height:99rpx;
  z-index:20;
  overflow: hidden;
}

.rank_items_userPic{
  position: absolute;
  left:2rpx;
  top:2rpx;
  width:76rpx;
  height:76rpx;
  border-radius: 38rpx;
  z-index:10;
}
.rank_items_third{
  margin-left:230rpx;
  margin-right:160rpx;
}
.rank_items_forth{
  position: absolute;
  top:0;
  right:0;
  text-align: center;
  width:150rpx;

}

.rank_items_bd:first-child .rank_items_userbg{
  background: url(data:../images/new/jin.png;base64,) no-repeat;
  background-size:100% 100%; 
}
.rank_items_bd:nth-child(2) .rank_items_userbg{
  background: url(data:../images/new/yin.png;base64,) no-repeat;
  background-size:100% 100%; 
}
.rank_items_bd:nth-child(3) .rank_items_userbg{
  background: url(data:../images/new/tong.png;base64,) no-repeat;
  background-size:100% 100%; 
}


.rank_pre{
  position: relative;
  width:calc( 100vw - 80rpx);
  /* height:140rpx;
  line-height:140rpx; */
  padding:10rpx 50rpx;
  background:#fff;
  margin-bottom:40rpx;
  
}
/* .rank_pre:nth-last-child(2n){
  background:#E1D676;
} */
.rank_pre:first-child{
  background:#E1948F;
}
.rank_pre:nth-child(2){
  background:#E1D676;
}
.rank_pre:nth-child(3){
  background:#7ACEE1;
}
.rank_pre>view:nth-child(1){
  position: absolute;
  left:20rpx;
}
.rank_pre>view:nth-child(2){
  margin-left:80rpx;
  margin-top:10rpx;
}
.rank_pre>view:nth-child(3)>view{
  width:220rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.rank_userPic{
  width:100rpx;
  height:100rpx;
  border-radius: 50rpx;
}
.rank_rankNum{
  width:100rpx;
  height:100rpx;
  line-height:100rpx;
  border-radius: 50rpx;
  font-size:40rpx;
  font-weight: 700;
  text-align: center;
}
.rank_pre:first-child .rank_rankNum,.rank_pre:nth-child(2) .rank_rankNum,.rank_pre:nth-child(3) .rank_rankNum{
  color:#fff;
}

